{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"required": ["firstName", "lastName", "email"],
	"properties": {
		"firstName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 15
		},
		"lastName": {
			"type": "string",
			"minLength": 1
		},
		"email": {
			"type": "string",
			"minLength": 6,
			"format": "email"
		}
	},
	"examples": [
		{
			"firstName": "Kyle",
			"lastName": "Pitts",
			"email": "kylepitts@mail.com"
		}
	]
}
